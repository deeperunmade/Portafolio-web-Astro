---
import React from "../components/icons/React.astro";
import AstroIcon from "../components/icons/AstroIcon.astro";
import Git from "../components/icons/Git.astro";
import Github from "../components/icons/Github.astro";
import MySQL from "../components/icons/MySQL.astro";
import Node from "../components/icons/Node.astro";
import Tailwind from "../components/icons/Tailwind.astro";
import Boostrap from "../components/icons/Boostrap.astro";
import Next from "./icons/Next.astro";
import Redux from "./icons/Redux.astro";
import TypeScript from "./icons/TypeScript.astro";
import SqlServer from "./icons/SqlServer.astro";
---
<div class="container text-center">
  <div class="row">
    <h2 class="mb-5">Mis Habilidades</h2>
    <div class="col-12">
      <div class="row gy-5">
        <div  title="React" class="col-2">
          <React />
        </div>
          <div  title="Astro" class="col-2">
            <AstroIcon />
          </div>
          <div  title="Git" class="col-2">
            <Git />
          </div>
          <div  title="TypeScript" class="col-2">
            <TypeScript />
          </div>
          <div  title="Github" class="col-2">
            <Github />
          </div>
          <div  title="Next.js" class="col-2">
            <Next />
          </div>
          <div  title="Redux" class="col-2">
            <Redux />
          </div>
          <div  title="MySQL" class="col-2">
            <MySQL />
          </div>
          <div  title="SQL Server" class="col-2">
            <SqlServer />
          </div>
          <div  title="Node.js" class="col-2">
            <Node />
          </div>
          <div  title="Tailwind CSS" class="col-2">
            <Tailwind />
          </div>
          <div  title="Boostrap" class="col-2">
            <Boostrap />
